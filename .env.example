# DashScope API Key（必填）
DASHSCOPE_API_KEY=your-dashscope-api-key-here

# 环境配置
NODE_ENV=development
LOG_LEVEL=debug

# 数据库配置
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=ai_gateway
DATABASE_USER=ai_gateway_user
DATABASE_PASSWORD=ai_gateway_password

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# RabbitMQ 配置
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=ai_gateway
RABBITMQ_PASSWORD=ai_gateway_password

# API Gateway 配置
GATEWAY_PORT=3000
GATEWAY_API_KEYS=sk-test-xxx,sk-prod-xxx

# Agent Orchestrator 配置
ORCHESTRATOR_PORT=3001

# Prompt Manager 配置
PROMPT_MANAGER_PORT=3002
